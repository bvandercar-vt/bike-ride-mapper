  "type": "module",
  "description": "MapMyRide Mapper",
  "main": "index.js",
  "homepage": "TODO.com",
  "scripts": {
    "start": "yarn dev",
    "dev": "vite --open",
    "prebuild": "yarn build:workouts",
    "build": "vite build",
    "preview": "yarn build && vite preview --open",
    "lint": "yarn format && yarn ts:check && yarn eslint:check && yarn format:check",
    "lint:fix": "yarn format && yarn eslint:fix",
    "ts:check": "tsc",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "get-data": "npx tsx ./scripts/get-workouts.ts",
    "get-mmr-token": "npx tsx ./scripts/get-mmr-token.ts",
    "build:workouts": "npx tsx ./scripts/build-workouts-ndjson.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bvandercar-vt/TODO"
  },
  "keywords": [
    "MapMyRide"
  ],
  "author": "Blake Vandercar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bvandercar-vt/TODO/issues"
  },
  "dependencies": {
    "classnames": "^2",
    "dotenv": "^16",
    "es-toolkit": "^1.44",
    "leaflet": "^1.9",
    "leaflet-polylinedecorator": "^1.6",
    "luxon": "^3.7",
    "ndjson-readablestream": "^1.4",
    "react": "^18",
    "react-dom": "^18",
    "react-leaflet": "^4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1",
    "@tmcw/togeojson": "^7",
    "@turf/simplify": "^7",
    "@types/leaflet": "^1.9",
    "@types/leaflet-polylinedecorator": "^1.6",
    "@types/luxon": "^3.4",
    "@types/node": "^24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/xmldom": "^0.1",
    "@vitejs/plugin-react": "^4",
    "autoprefixer": "^10",
    "eslint": "^9",
    "eslint-config-prettier": "^9",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react": "^7",
    "postcss": "^8",
    "postcss-load-config": "^6",
    "prettier": "^3",
    "tailwindcss": "^4.1",
    "tsx": "^4.20.3",
    "type-fest": "^5",
    "typescript": "^5",
    "typescript-eslint": "^8",
    "vite": "^5.4",
    "xmldom": "^0.6"
  }
